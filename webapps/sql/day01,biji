  create table t2(name varchar(10),age int)engine=myisam charset=gbk;
  show tables;
  show create table t1;
  desc t1;
  show  create table t2;
  desc t2;
  drop table t1;
  show tables;
  drop table t2;
  show tables;
  drop database db4;
  show databases;
  
  
