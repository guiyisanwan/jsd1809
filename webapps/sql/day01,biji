  create table t2(name varchar(10));
  rename table t2 to t3;
  alter table t3 engine =myisam charset=gbk;
  show create table t3;
  alter table t3 add age int;
  desc t3;
  alter table  t3 add id int first;
  desc t3;
  alter table t3 add sal int after name;
  desc t3;
  alter table t3 drop sal;
  desc t3;
  alter table t3 change name username varchar(5);
  desc t3;
  alter table t3 modify age int after id;
  desc t3;
  
